      "launchBrowser": false,  // birden fazla ekran açılmaması ve tarayıcıdan açmamak için false yaparız.
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:5011",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
